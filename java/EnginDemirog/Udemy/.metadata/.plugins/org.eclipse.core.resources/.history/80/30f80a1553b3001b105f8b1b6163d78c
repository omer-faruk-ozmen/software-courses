package throwDemo;

public class AccountMenager {
	private double balance;

	public void deposit(double amount) {
		balance += amount;
	}

	public void withdraw(double amount) {
		if(balance >= amount) {
			balance -= amount;
		}else {
			throw new Exception("Bakiye yetersiz")
		}
		
		
	}

	public double getBalance() {
		return balance;
	}

}
